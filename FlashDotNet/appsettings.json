{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:9000"
      }
    }
  },
  // 可选值：mysql, sqlite, sqlserver, postgresql
  // 默认值：sqlite
  // 为空默认在程序根目录下创建App.db
  "DefaultConnection": "sqlite",
  "ConnectionStrings": {
    "MySqlConnection": "server=127.0.0.1;database=FlashDotNet;user=root;password=qwertyuiop",
    "SqlServerConnection": "Server=127.0.0.1;Database=FlashDotNet;User Id=myUsername;Password=myPassword;",
    "SqliteConnection": "Data Source=App.db",
    "PostgreSqlConnection": "Host=127.0.0.1;Database=FlashDotNet;Username=myUsername;Password=myPassword"
  },
  "TokenOptions": {
    "SecretKey": "[输入你的鉴权Key 随便填]",
    "Issuer": "[输入发行者，随便填]",
    "Audience": "[输入Audience，随便填]",
    "ExpireMinutes": 30
  }
}
